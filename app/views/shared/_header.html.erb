<% cache('header') do -%>
<div class="logo">
  <%= link_to image_tag('logo2.gif', :alt => RefinerySetting.find_or_set(:site_name, "Strange Loop")) , root_path %>
</div>
<%= render "/shared/menu", :dom_id => 'menu', :css => 'menu' -%>

<% end -%>
<%= render '/shared/current_attendee', :username => current_attendee.try(:full_name),
  :attendee_path => (current_attendee ? attendee_path(current_attendee) : ''),
  :login_path => new_attendee_cred_session_path,
  :logout_path => destroy_attendee_cred_session_path %>
